{
  "productName": "secret-sharing-app",
  "version": "0.0.1",
  "identifier": "me.binau.dev.secret-sharing-app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run generate",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Secret Sharing",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
